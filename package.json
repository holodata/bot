{
  "name": "@holodata/bot",
  "description": "Discord bot for @holodata",
  "version": "0.0.0",
  "author": "Yasuaki Uechi <y@uechi.io> (https://uechi.io/)",
  "scripts": {
    "build": "npm run genparser && tsc",
    "clean": "shx rm -rf dist",
    "deploy-commands": "ts-node ./src/deploy-commands.ts",
    "dev": "run-p dev:*",
    "dev:server": "nodemon -w dist .",
    "dev:tsc": "tsc -w",
    "genparser": "peggy ./src/honeyql/index.pegjs",
    "predev": "npm run deploy-commands",
    "prepare": "husky install",
    "start": "node .",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "@typegoose/typegoose": "^9.7.0",
    "discord.js": "^13.6.0",
    "mongoose": "^6.2.4",
    "peggy": "^1.2.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.40",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "shx": "^0.3.4",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "homepage": "https://github.com/holodata/honeybee-bot",
  "repository": {
    "type": "git",
    "url": "https://github.com/holodata/honeybee-bot.git"
  },
  "bugs": {
    "url": "https://github.com/holodata/honeybee-bot/issues"
  },
  "license": "MIT",
  "keywords": [
    "honeybee-bot"
  ],
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  }
}
